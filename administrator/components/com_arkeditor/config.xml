				<field 
			name="custom-script2"
			type="script"
			script="jQuery(function(){ jQuery('#jform_default_exclude_stylesheet_chzn').css('width','100%');  jQuery('#jform_default_exclude_selectors_chzn').css('width','100%');});" />
			
		<field name="stylelistsource" 
		type="list" 
		default="1" 
		onchange="var arkswitcher={};switch(arkswitcher.a=jQuery(this).val(),arkswitcher.b=jQuery('#jform_arkcustomtypographycontent'),arkswitcher.c=jQuery('#jform_arkcustomtypographycontent+br+br+input'),arkswitcher.d=jQuery('#jform_arkcustomtypographyfile'),parseInt(arkswitcher.a)){case 2:arkswitcher.b.attr('readonly',!1),arkswitcher.c.attr('disabled',!1),arkswitcher.d.attr('readonly',!0);break;case 3:arkswitcher.b.attr('readonly',!0),arkswitcher.c.attr('disabled',!0),arkswitcher.d.attr('readonly',!1);break;default:arkswitcher.b.attr('readonly',!0),arkswitcher.c.attr('disabled',!0),arkswitcher.d.attr('readonly',!0)}"
		label="COM_ARKEDITOR_FIELD_STYLELISTSOURCE"
		description="COM_ARKEDITOR_FIELD_STYLELISTSOURCE_DESC"
		>
			<option value="1">ARKSTYLESHEETPARSER</option>
			<option value="2">ARKCUSTOM</option>
			<option value="3">ARKCUSTOMFILE</option>
		</field>	
		
		<field name="customcssswitcher" 
			type="listswitcher" 
			display="['bootstrap2', 'bootstrap3', 'bootstrap3gantry5', 'uikit2', 'uikit3', 'arkuikit']"
			target="arkcustomtypographycontent"
			label ="COM_ARKEDITOR_FIELD_CUSTOM_CSS_SWITCHER"
			default="0"
			showon="stylelistsource:2"
		>
